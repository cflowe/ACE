idl_stubs: $(ADDITIONAL_IDL_TARGETS)

# This assignment forces make to run the idl_stubs
# target before building any of the source files.
FORCED_IDL_STUBS = Foo_AC.cpp Foo_AS.cpp Foo_A_ClientEngine.cpp Foo_A_i.cpp Foo_A_Statistics.cpp  
FORCED_IDL_STUBS := $(FORCED_IDL_STUBS:Foo_AS.cpp=)
FORCED_IDL_STUBS := $(FORCED_IDL_STUBS:Foo_AC.cpp=)

ifneq ($(FORCED_IDL_STUBS),)
$(FORCED_IDL_STUBS): idl_stubs
endif


realclean: clean
