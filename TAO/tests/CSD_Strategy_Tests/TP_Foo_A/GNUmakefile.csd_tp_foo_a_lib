LIB_CHECKED      = $(LIB_UNCHECKED)
LIB_NAME         = $(LIB_PREFIX)CSD_TP_Foo_A

## SHLIB may be set to empty later on in this file
SHLIB_UNCHECKED  = $(LIB_PREFIX)CSD_TP_Foo_A.$(SOEXT)
SHLIB            = $(SHLIB_UNCHECKED)

