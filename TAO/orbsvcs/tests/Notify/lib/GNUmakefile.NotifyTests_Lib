GENERATED_DIRTY += Activation_ManagerC.inl Activation_ManagerS.h Activation_ManagerC.h Activation_ManagerS.cpp Activation_ManagerC.cpp
OBJS_DEPEND_ON_GENERATED = 1
## More than one file is generated by the command and therefore
## it can not be run in parallel.  Unfortunately, there is no way to
## say that only this rule can't be run in parallel.  However, we can
## determine if the generated files have already been generated.  If that's
## the case, then we don't need this special rule.
ifeq ($(wildcard $(GENERATED_DIRTY)), $(GENERATED_DIRTY))
  ## If we can find /bin/test, then we will continue
  ifneq ($(TEST_EXE),)
    ## If all of the generated files are there, then we need to check
    ## and make sure that the generated files are up-to-date.  If they are not
    ## then we need the special rule.
    ifeq ($(shell $(TEST_EXE) "Activation_Manager.idl" -nt "Activation_ManagerC.inl" 2> /dev/null || $(TEST_EXE) "Activation_Manager.idl" -nt "Activation_ManagerS.h" 2> /dev/null || $(TEST_EXE) "Activation_Manager.idl" -nt "Activation_ManagerC.h" 2> /dev/null || $(TEST_EXE) "Activation_Manager.idl" -nt "Activation_ManagerS.cpp" 2> /dev/null || $(TEST_EXE) "Activation_Manager.idl" -nt "Activation_ManagerC.cpp" 2> /dev/null || echo 0),)
      .NOTPARALLEL:

    else
      ## By this point, all of the generated files are here and up-to-date
      ## with respect to the source file.  Now we need to make sure that
      ## they are up-to-date with respect to the generation tool.  If the tool
      ## is newer than the generated files, then we need the special rule.
      ifeq ($(shell $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Activation_ManagerC.inl" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Activation_ManagerS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Activation_ManagerC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Activation_ManagerS.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Activation_ManagerC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Activation_ManagerC.inl" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Activation_ManagerS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Activation_ManagerC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Activation_ManagerS.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Activation_ManagerC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Activation_ManagerC.inl" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Activation_ManagerS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Activation_ManagerC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Activation_ManagerS.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Activation_ManagerC.cpp" 2> /dev/null || echo 0),)
        .NOTPARALLEL:
      endif
    endif
  else
    .NOTPARALLEL:
  endif
else
.NOTPARALLEL:
endif
Activation_ManagerC.inl Activation_ManagerS.h Activation_ManagerC.h Activation_ManagerS.cpp Activation_ManagerC.cpp: Activation_Manager.idl $(subst $(SPACE),\$(SPACE),$(call ADD_ARCH,$(TAO_IDL_DEP)))
	$(if $(findstring ",$(TAO_IDL)),$(TAO_IDL),$(call ADD_QUOTE,$(call ADD_ARCH,$(TAO_IDL)))) $(TAO_IDLFLAGS) -I../../../../orbsvcs Activation_Manager.idl


ifneq ($(static_libs_only),1)
ifneq ($(CROSS-COMPILE),1)
Activation_ManagerC.inl Activation_ManagerS.h Activation_ManagerC.h Activation_ManagerS.cpp Activation_ManagerC.cpp: $(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT)) $(call ADD_ARCH,../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))
endif
endif

