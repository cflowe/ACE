# turn off libcheck if doing a dry run
ifeq ($(findstring n, $(MAKEFLAGS)),n)
  LIBCHECK = 1
else
  # turn off libcheck if keep going was passed too
  ifeq ($(findstring k, $(MAKEFLAGS)),k)
    LIBCHECK = 1
  else
    LIBCHECK ?= $(filter-out $(foreach lib,TAO_NotifyTests TAO_CosEvent_Serv TAO_IFR_Client TAO_DynamicInterface TAO_CosNaming TAO_CosNotification_Serv TAO_Messaging TAO_DynamicAny TAO_Valuetype ACE_ETCL_Parser TAO_ETCL ACE_ETCL TAO_Svc_Utils TAO_CosNotification_Skel TAO_CosEvent_Skel TAO_RTPortableServer TAO_PortableServer TAO_RTCORBA TAO_PI TAO_CodecFactory TAO_CosNotification TAO_CosEvent TAO_AnyTypeCode TAO ACE,$(findstring $(lib),$(foreach libpath,. $(if $(ARCH),./$(ARCH)) ../../../../../lib $(if $(ARCH),../../../../../lib/$(ARCH)) ../lib $(if $(ARCH),../lib/$(ARCH)) ../../lib $(if $(ARCH),../../lib/$(ARCH)) /usr/lib /usr/lib64 $(INSLIB),$(wildcard $(libpath)/$(LIB_PREFIX)$(lib).* $(libpath)/$(lib).lib)))),TAO_NotifyTests TAO_CosEvent_Serv TAO_IFR_Client TAO_DynamicInterface TAO_CosNaming TAO_CosNotification_Serv TAO_Messaging TAO_DynamicAny TAO_Valuetype ACE_ETCL_Parser TAO_ETCL ACE_ETCL TAO_Svc_Utils TAO_CosNotification_Skel TAO_CosEvent_Skel TAO_RTPortableServer TAO_PortableServer TAO_RTCORBA TAO_PI TAO_CodecFactory TAO_CosNotification TAO_CosEvent TAO_AnyTypeCode TAO ACE)
    ifeq ($(LIBCHECK),)
      LIBCHECK = 1
    endif
  endif
endif
