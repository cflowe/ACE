ifneq ($(CROSS-COMPILE),1)
RTCORBA_includeC.h RTCORBA_includeA.h RTCORBA_includeS.h RTCORBA_includeC.cpp: $(call ADD_ARCH,../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT)) $(call ADD_ARCH,../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))
endif
endif

